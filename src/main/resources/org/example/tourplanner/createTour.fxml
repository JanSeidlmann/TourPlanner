      <Label layoutX="28.0" layoutY="224.0" text="Transport:" />
      <ChoiceBox fx:id="transportType" layoutX="91.0" layoutY="220.0" prefWidth="150.0" />
      <Button fx:id="createButton" layoutX="534.0" layoutY="361.0" mnemonicParsing="false" onAction="#onCreateButtonClicked" text="Create" />
      <Button fx:id="cancelButton" layoutX="478.0" layoutY="361.0" mnemonicParsing="false" onAction="#onCancelButtonClicked" text="Cancel" />

   </children>
</AnchorPane>
