<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane minHeight="400.0" minWidth="600.0" styleClass="pane" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.tourplanner.PL.controller.CreateTourController">

   <children>
      <Label fx:id="labelNewWindow" layoutX="223.0" prefHeight="45.0" prefWidth="140.0" text="Create Tour">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="28.0" layoutY="70.0" text="Name:" />
      <TextField fx:id="nameTextField" layoutX="66.0" layoutY="66.0" prefHeight="25.0" prefWidth="467.0" />
      <Label layoutX="28.0" layoutY="100.0" text="Description:" />
      <TextField fx:id="descriptionTextField" layoutX="94.0" layoutY="96.0" prefHeight="59.0" prefWidth="440.0" />
      <Label layoutX="28.0" layoutY="164.0" text="From:" />
      <TextField fx:id="routeInformationTextField" layoutX="140.0" layoutY="310.0" prefHeight="25.0" prefWidth="392.0" />
      <Label layoutX="28.0" layoutY="194.0" text="To:" />
      <TextField fx:id="timeTextField" layoutX="66.0" layoutY="280.0" prefHeight="25.0" prefWidth="467.0" />
      <Label layoutX="28.0" layoutY="254.0" text="Distance:" />
